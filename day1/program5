a=int(input("run by first player"))
b=int(input("run by second player"))
c=int(input("run by third player"))
strike_rate1=(a/60)*100
strike_rate2=(b/60)*100
strike_rate3=(c/60)*100
print("strike rates are: ",strike_rate1,strike_rate2,strike_rate3)
print("new strike rates are",strike_rate1*2,strike_rate2*2,strike_rate3*2)
print("maximum number of sixes are: ",a//6,b//6,c//6)
