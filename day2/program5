for i in range(3):
    for j in range(4-i*2,-1,-1):
        if(j%2==0):
            print(3-i,end="")
        else:
            print("*",end="")
    print(" ")        
    
for i in range(3):
    for j in range(0,2*i+1):
        if(j%2==0):
            print(i+1,end="")
            
        else:
            print("*",end="")
    print(" ")        
            
