for i in range(0,5):
    for j in range(0,5-i):
        print("*",end="")
    for j in range(0,2*i+1):
        print(" ",end="")
    for j in range(i,5):
        print("*",end="")
    print()
for i in range(0,4):
    for j in range(0,i+2):
        print("*",end="")
    for j in range(0,2*(4-i)-1):
        print(" ",end="")
    for j in range(4,2-i,-1):
        print("*",end="")
    print()    
